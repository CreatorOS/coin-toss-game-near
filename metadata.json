{
  "version": "1.0.0",
  "defaultFile": "src/cointoss/assembly/index.ts",
  "environment": "near-asm",
  "setupCommands": [". scripts/1.init.sh cointoss.wasm"],
  "compileCommand": "yarn build:release || exit 1",
  "tests": [
    {
      "itShould": "creates a new game",
      "command": ". learn_tests/01.sh"
    },
    {
      "itShould": "Return the details of a newly created game",
      "command": ". learn_tests/02.sh"
    },
    {
      "itShould": "A Player 2 joining an existing game",
      "command": ". learn_tests/03.sh"
    },
    {
      "itShould": "Sad paths - failures",
      "command": ". learn_tests/04.sh"
    }
  ],
  "learnerAccessibleFileList": ["src/cointoss/assembly/*"]
}
